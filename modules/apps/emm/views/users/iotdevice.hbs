<div class="container">

	<div class="raw-fluid">
		<div class="span12">
			<div class="raw-fluid">
				<ul class="nav nav-pills">
					<li>
						<a href="/emm/users/tokens"><i class='icon-money'></i> Tokens</a>
					</li>
					<li class="active">
						<a href="/emm/users/iotdevice"><i class='icon-rocket'></i> Devices</a>
					</li>
				</ul>
			</div>
		</div>
	</div>

	<div class="raw">
		<div class="span12">
			<div class="raw-fluid">

				<div class="block">
					<div class="block-header">
						<div class="block-header-title pull-left">
							Unclaimed Devices
						</div>
					</div>

					<div class="block-body block-body container-fluid raw-fluid">

						<div class="span11">
							<table cellpadding="0" cellspacing="0" border="0"
							class="table table-striped table-bordered" id="main-table">
								<thead>
									<tr>
										<th>ID</th>
										<th>Vendor</th>
										<th></th>
									</tr>
								</thead>
								<tbody>
								{{#each data.devices}}
									<tr>
										<td>{{id}}</td>
										<td>{{vendor}}</td>
										<td><button class="claim" data-button="{{id}}">Claim</button></td>
									</tr>
								{{/each}}
									
								</tbody>
							</table>

						</div>

					</div>

				</div>

			</div>

		</div>

	</div>

</div>

